import random

n = int(input("lucky Drawer Enter your number(1/100) :"))
print(" ")
print("    This are the 10 lucky numbers")
l = []

while True:
    if len(l)<10: 
        x = random.randint(1,100)
        if x not in l:
            l.append(x)
    if len(l) == 10:
        break
print(l)
print(" ")
if n in l:
    print("YOU WIN")
else:
    print("YOU LOSE")
